{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/recommended"
  ],
  globals: {
    globalThis: true
  },
  parserOptions: {
    ecmaVersion: 2022,
    sourceType: "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-console": 0,
    "no-empty": 0
  }
}
